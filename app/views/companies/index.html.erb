<h1>Input Ticker to Build a Comparables Table</h1>

<%= form_tag "/companies", method: :get do %>
	<%= text_field_tag :query %>
	<%= submit_tag "Search"%> 		

<% end %>

<% @companies.each do |company| %>
	<p>
		Name: <%= link_to company.name, company_path(company) %><br />
		Ticker: <%= company.ticker %><br />	
	</p>
<% end %>	

